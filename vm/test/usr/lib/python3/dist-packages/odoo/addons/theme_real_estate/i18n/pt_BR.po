#. module: theme_real_estate
#: model_terms:theme.ir.ui.view,arch:theme_real_estate.s_banner
msgid "<b>Real Estate Agency.<br/>Near You.</b>"
msgstr "<b>Imobiliárias<br/>perto de você.</b>"

#. module: theme_real_estate
#: model_terms:theme.ir.ui.view,arch:theme_real_estate.s_three_columns
msgid "<span class=\"text-600 fw-bold\">$1,295,000,000</span>"
msgstr "<span class=\"text-600 fw-bold\">US$ 1.295.000.000</span>"

#. module: theme_real_estate
#: model_terms:theme.ir.ui.view,arch:theme_real_estate.s_three_columns
