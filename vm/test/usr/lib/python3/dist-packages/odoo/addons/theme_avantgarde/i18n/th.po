#: model_terms:theme.ir.ui.view,arch:theme_avantgarde.s_three_columns
msgid "<span style=\"font-weight: bolder;\">Collaboration</span>"
msgstr "<span style=\"font-weight: bolder;\">การทำงานร่วมกัน</span>"

